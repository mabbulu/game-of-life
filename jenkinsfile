node {
     stage ('scm') {
           git 'https://github.com/mabbulu/game-of-life.git'
     }
     stage ('Package') {
	   sh 'mvn package'
     }
     stage ('Archve') {
           archiveArtifacts 'gameoflfe-web/target/*.war'
     }
     stage ('Reports') {
           junit 'gameoflfe-web/target/surefire-reports/*.xml'
     }

}
